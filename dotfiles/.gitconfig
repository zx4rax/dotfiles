[color]
  diff = auto
  status = auto
  branch = auto
[color "status"]
  added = green
  changed = blue
  untracked = red 
[color "diff"]
  meta = yellow
  old = red 
  new = green
[alias]
  st = status
  ci = commit
  co = checkout
  tm = merge --no-ff --no-commit
  pp = !git pull && git push
[core]
  editor = /usr/bin/vim
  mergeoptions = 
[push]
  default = current
[giggle]
  main-window-maximized = false
  main-window-geometry = 1269x839+117+89
